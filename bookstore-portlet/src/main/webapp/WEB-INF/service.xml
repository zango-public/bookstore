<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 6.2.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_6_2_0.dtd">

<service-builder package-path="com.zango">
	<namespace>bookstore</namespace>
	<entity name="Books" uuid="true" local-service="true" remote-service="true">
		<column name="bookId" type="long" primary="true"></column>
		<column name="bookName" type="String"></column>
		<column name="bookAuthor" type="String"></column>
		<column name="bookResume" type="String"></column>		
	</entity>
</service-builder>